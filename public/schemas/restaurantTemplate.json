{
  "@context": "https://schema.org",
  "@type": "Restaurant",
  "name": "{{name}}",
  "description": "{{description}}",
  "url": "https://www.tennsational.com/#/restaurants?id={{id}}",
  "telephone": "{{phone}}",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "{{address}}",
    "addressLocality": "{{city}}",
    "addressRegion": "TN",
    "postalCode": "{{zipCode}}",
    "addressCountry": "US"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": "{{latitude}}",
    "longitude": "{{longitude}}"
  },
  "image": "{{imageUrl}}",
  "priceRange": "{{priceRange}}",
  "servesCuisine": ["{{cuisine}}"],
  "menu": "{{menuUrl}}",
  "acceptsReservations": "{{acceptsReservations}}",
  "hasMenu": {
    "@type": "Menu",
    "name": "{{name}} Menu",
    "description": "Menu for {{name}} in {{city}}, TN",
    "hasMenuSection": [
      {
        "@type": "MenuSection",
        "name": "Signature Dishes",
        "description": "Most popular dishes at {{name}}",
        "hasMenuItem": [
          {
            "@type": "MenuItem",
            "name": "{{signatureDish1}}",
            "description": "{{signatureDish1Description}}",
            "offers": {
              "@type": "Offer",
              "price": "{{signatureDish1Price}}",
              "priceCurrency": "USD"
            }
          },
          {
            "@type": "MenuItem",
            "name": "{{signatureDish2}}",
            "description": "{{signatureDish2Description}}",
            "offers": {
              "@type": "Offer",
              "price": "{{signatureDish2Price}}",
              "priceCurrency": "USD"
            }
          }
        ]
      }
    ]
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "{{rating}}",
    "reviewCount": "{{reviewCount}}",
    "bestRating": "5",
    "worstRating": "1"
  },
  "review": [
    {
      "@type": "Review",
      "author": {
        "@type": "Person",
        "name": "{{reviewAuthor1}}"
      },
      "datePublished": "{{reviewDate1}}",
      "reviewRating": {
        "@type": "Rating",
        "ratingValue": "{{reviewRating1}}",
        "bestRating": "5",
        "worstRating": "1"
      },
      "reviewBody": "{{reviewBody1}}"
    },
    {
      "@type": "Review",
      "author": {
        "@type": "Person",
        "name": "{{reviewAuthor2}}"
      },
      "datePublished": "{{reviewDate2}}",
      "reviewRating": {
        "@type": "Rating",
        "ratingValue": "{{reviewRating2}}",
        "bestRating": "5",
        "worstRating": "1"
      },
      "reviewBody": "{{reviewBody2}}"
    }
  ],
  "openingHoursSpecification": [
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Monday"],
      "opens": "{{mondayOpen}}",
      "closes": "{{mondayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Tuesday"],
      "opens": "{{tuesdayOpen}}",
      "closes": "{{tuesdayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Wednesday"],
      "opens": "{{wednesdayOpen}}",
      "closes": "{{wednesdayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Thursday"],
      "opens": "{{thursdayOpen}}",
      "closes": "{{thursdayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Friday"],
      "opens": "{{fridayOpen}}",
      "closes": "{{fridayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Saturday"],
      "opens": "{{saturdayOpen}}",
      "closes": "{{saturdayClose}}"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Sunday"],
      "opens": "{{sundayOpen}}",
      "closes": "{{sundayClose}}"
    }
  ],
  "paymentAccepted": "{{paymentMethods}}",
  "smokingAllowed": "{{smokingAllowed}}",
  "amenityFeature": [
    {
      "@type": "LocationFeatureSpecification",
      "name": "Outdoor Seating",
      "value": "{{hasOutdoorSeating}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Wheelchair Accessible",
      "value": "{{wheelchairAccessible}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Delivery",
      "value": "{{offersDelivery}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Takeout",
      "value": "{{offersTakeout}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Reservations",
      "value": "{{acceptsReservations}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Serves Alcohol",
      "value": "{{servesAlcohol}}"
    },
    {
      "@type": "LocationFeatureSpecification",
      "name": "Wi-Fi",
      "value": "{{hasWifi}}"
    }
  ],
  "specialty": ["{{specialty1}}", "{{specialty2}}", "{{specialty3}}"],
  "keywords": ["{{keyword1}}", "{{keyword2}}", "{{keyword3}}", "{{keyword4}}", "{{keyword5}}"],
  "publicAccess": true,
  "isAccessibleForFree": true,
  "currenciesAccepted": "USD",
  "areaServed": {
    "@type": "City",
    "name": "{{city}}",
    "containedInPlace": {
      "@type": "AdministrativeArea",
      "name": "{{county}} County"
    }
  },
  "potentialAction": {
    "@type": "ReviewAction",
    "target": {
      "@type": "EntryPoint",
      "urlTemplate": "https://www.tennsational.com/#/restaurants?id={{id}}&action=review"
    }
  },
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "https://www.tennsational.com/#/restaurants?id={{id}}"
  }
}
